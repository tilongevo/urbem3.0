            <list-property name="resultSet"/>
            <xml-property name="queryText"><![CDATA[ SELECT  anda.cod_assunto,
         anda.descricao,
         anda.ordem,
         (
			SELECT  descricao
			  FROM  organograma.orgao_descricao
			 WHERE  orgao_descricao.cod_orgao = orgao.cod_orgao
		  ORDER BY  timestamp DESC
			 LIMIT  1
		 ) as nom_setor

   FROM  sw_andamento_padrao as anda,
         organograma.orgao

  WHERE  anda.cod_orgao = orgao.cod_orgao
    AND  cod_assunto = ?
    AND  cod_classificacao = ?
    ORDER BY anda.ordem]]></xml-property>
