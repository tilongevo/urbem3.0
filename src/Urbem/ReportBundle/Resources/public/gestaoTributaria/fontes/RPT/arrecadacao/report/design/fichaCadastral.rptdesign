            <xml-property name="queryText"><![CDATA[SELECT  numcgm
     , nom_cgm	
     , (logradouro||', '||numero||'/'||complemento||' - '||bairro||', '||nom_municipio||'/'||sigla_uf) as endereco
     , al.cod_lancamento
     , al.total_parcelas
     , al.valor
     , alc.dt_lancamento	
     , ap.vencimento 
     , btrim ( arrecadacao.fn_busca_lancamento_situacao( al.cod_lancamento ) ) as situacao			
     , case when acgc.cod_grupo IS NULL then
         calculo.cod_credito||'.'||calculo.cod_natureza||'.'||
         calculo.cod_genero||'.'||calculo.cod_especie||' - '||	
	      (SELECT descricao_credito
		 FROM monetario.credito
		WHERE cod_credito  = calculo.cod_credito
		  AND cod_natureza = calculo.cod_natureza
		  AND cod_genero   = calculo.cod_genero
		  AND cod_especie  = calculo.cod_especie)
       ELSE acgc.cod_grupo||'/'||acgc.ano_exercicio||' - '||
       	( SELECT descricao
	 	FROM arrecadacao.grupo_credito
	       WHERE cod_grupo = acgc.cod_grupo
		 AND ano_exercicio = acgc.ano_exercicio )
       END as desc_credito

   FROM arrecadacao.calculo
  INNER JOIN arrecadacao.calculo_cgm
    USING( cod_calculo )
  INNER JOIN sw_cgm
    USING( numcgm )
  INNER JOIN sw_municipio
    USING( cod_municipio, cod_uf) 
  INNER JOIN sw_uf
    USING( cod_uf)

  INNER JOIN arrecadacao.lancamento_calculo as alc
     ON calculo.cod_calculo = alc.cod_calculo
  INNER JOIN arrecadacao.lancamento as al
     ON alc.cod_lancamento = al.cod_lancamento
    AND al.divida = 'f'

  LEFT JOIN (SELECT cod_lancamento, MIN(vencimento) as vencimento
	       FROM arrecadacao.parcela
	      WHERE (nr_parcela = 0 OR nr_parcela = 1)
		GROUP By cod_lancamento) as ap
    ON alc.cod_lancamento = ap.cod_lancamento  

  LEFT JOIN arrecadacao.calculo_grupo_credito as acgc
    ON acgc.cod_calculo = calculo.cod_calculo
]]></xml-property>
