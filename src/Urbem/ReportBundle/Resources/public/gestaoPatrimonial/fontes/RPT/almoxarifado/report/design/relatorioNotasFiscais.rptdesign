    <data-sets>
        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="dsNotasFiscais" id="6672">
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[this.queryText  = "SELECT 									 				 \n";
this.queryText += "        sw_cgm.nom_cgm as nome_fornecedor 				 \n";
this.queryText += "        , nota_fiscal_fornecedor.num_nota  as numero_nota \n";
this.queryText += "        , nota_fiscal_fornecedor.num_serie as numero_serie_nota \n";
this.queryText += "        , TO_CHAR(nota_fiscal_fornecedor.dt_nota,'dd/mm/yyyy') as data_nota \n";
this.queryText += "        , lancamento_material.cod_item as cod_item_nota  \n";
this.queryText += "        , catalogo_item.descricao as descricao 			\n";
this.queryText += "        , lancamento_material.quantidade as quantidade   \n";
this.queryText += "        , CAST((lancamento_material.valor_mercado/lancamento_material.quantidade) AS NUMERIC(14,2)) as valor_unitario \n";
this.queryText += "        , lancamento_material.valor_mercado as valor_total_item \n";
this.queryText += "FROM compras.nota_fiscal_fornecedor \n";

this.queryText += "INNER JOIN compras.fornecedor \n";
this.queryText += "        ON fornecedor.cgm_fornecedor = nota_fiscal_fornecedor.cgm_fornecedor \n";

this.queryText += "INNER JOIN sw_cgm \n";
this.queryText += "        ON sw_cgm.numcgm = fornecedor.cgm_fornecedor \n";

this.queryText += "INNER JOIN almoxarifado.natureza_lancamento \n";
this.queryText += "        ON natureza_lancamento.exercicio_lancamento = nota_fiscal_fornecedor.exercicio_lancamento \n";
this.queryText += "        AND natureza_lancamento.num_lancamento      = nota_fiscal_fornecedor.num_lancamento 		 \n";
this.queryText += "        AND natureza_lancamento.cod_natureza        = nota_fiscal_fornecedor.cod_natureza 		 \n";
this.queryText += "        AND natureza_lancamento.tipo_natureza       = nota_fiscal_fornecedor.tipo_natureza 		 \n";

this.queryText += "INNER JOIN almoxarifado.lancamento_material  \n";
this.queryText += "        ON lancamento_material.exercicio_lancamento = natureza_lancamento.exercicio_lancamento \n";
this.queryText += "        AND lancamento_material.num_lancamento      = natureza_lancamento.num_lancamento  	  \n";
this.queryText += "        AND lancamento_material.cod_natureza        = natureza_lancamento.cod_natureza         \n";
this.queryText += "        AND lancamento_material.tipo_natureza       = natureza_lancamento.tipo_natureza        \n";

this.queryText += "INNER JOIN almoxarifado.catalogo_item \n";
this.queryText += "        ON catalogo_item.cod_item = lancamento_material.cod_item \n";

this.queryText += params['stFiltro'];

this.queryText += "ORDER BY dt_nota \n";

params['query'] = this.queryText;


]]></method>
