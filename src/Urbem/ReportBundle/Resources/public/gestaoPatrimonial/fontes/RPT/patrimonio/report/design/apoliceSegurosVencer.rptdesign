    <data-sets>
        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="rsApoliceSegurosVencer" id="1149">
            <list-property name="columnHints"/>
            <list-property name="parameters"/>
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[// Filtra pelo intervalo de datas no SQL.
this.queryText += " bem_comprado.cod_entidade = "+params['entidade']+"";
if (params['dt_inicial'] != ''){
  this.queryText += " AND dt_vencimento BETWEEN TO_DATE('"+params['dt_inicial']+"', 'dd/mm/yyyy') AND TO_DATE('"+params['dt_final']+"', 'dd/mm/yyyy')";
}

// Seta o ORDER BY do SQL.
if (params['ordenacao'] != ''){
  this.queryText += ' ORDER BY ' + params['ordenacao'];
}

params['query'] = this.queryText;]]></method>
            <property name="dataSource">postgres</property>
            <list-property name="resultSet"/>
            <xml-property name="queryText"><![CDATA[SELECT DISTINCT
	a.cod_apolice, 
	c.nom_cgm, 
	a.num_apolice, 
	a.dt_vencimento,
	c.nom_cgm
FROM
	patrimonio.apolice as a
JOIN sw_cgm as c
  ON a.numcgm = c.numcgm
JOIN patrimonio.apolice_bem
  ON apolice_bem.cod_apolice = a.cod_apolice
JOIN patrimonio.bem_comprado
  ON bem_comprado.cod_bem = apolice_bem.cod_bem
WHERE ]]></xml-property>
        </oda-data-set>
    </data-sets>
