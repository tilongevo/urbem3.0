                    <property name="dataType">float</property>
                    <property name="nativeDataType">8</property>
                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[SELECT  EXTRACT(day FROM current_date) AS dia,
		CASE EXTRACT(month FROM current_date)
			WHEN 1 THEN  'Janeiro' 
			WHEN 2 THEN  'Fevereiro' 
			WHEN 3 THEN  'Março' 
			WHEN 4 THEN  'Abril'
			WHEN 5 THEN  'Maio'
			WHEN 6 THEN  'Junho'
			WHEN 7 THEN  'Julho'
			WHEN 8 THEN  'Agosto'
			WHEN 9 THEN  'Setembro'
			WHEN 10 THEN 'Outubro'
			WHEN 11 THEN 'Novembro'
			WHEN 12 THEN 'Dezembro'
		END	AS mes,
		EXTRACT(year FROM current_date) AS ano]]></xml-property>
        </oda-data-set>
