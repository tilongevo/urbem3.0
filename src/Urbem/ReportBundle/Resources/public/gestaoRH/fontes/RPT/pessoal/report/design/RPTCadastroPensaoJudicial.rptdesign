            <method name="beforeOpen"><![CDATA[if(params["stTipoFiltro"] == "atributo_servidor_grupo"){
	if(params['boAgrupar'] == 'true'){
		this.queryText += " , CASE WHEN atributo_valor_padrao.valor_padrao IS NOT NULL\n"; 
		this.queryText += "        THEN atributo_valor_padrao.valor_padrao\n";
		this.queryText += "        ELSE atributo_contrato_servidor_valor.valor\n"; 
		this.queryText += "    END AS atributo\n";
	}else{
		this.queryText += " , '' as atributo\n";
	}
}else{
	this.queryText += " , '' as atributo\n";
}

this.queryText += "      FROM folhapagamento.registro_evento_periodo\n";
this.queryText += "INNER JOIN folhapagamento.evento_calculado_dependente\n";
this.queryText += "        ON registro_evento_periodo.cod_registro = evento_calculado_dependente.cod_registro\n";
this.queryText += "INNER JOIN pessoal.dependente\n";
this.queryText += "        ON evento_calculado_dependente.cod_dependente = dependente.cod_dependente\n";
this.queryText += "INNER JOIN pessoal.servidor_dependente\n";
this.queryText += "        ON dependente.cod_dependente = servidor_dependente.cod_dependente\n";
this.queryText += "INNER JOIN pessoal.pensao\n";
this.queryText += "        ON servidor_dependente.cod_dependente = pensao.cod_dependente\n";
this.queryText += "       AND servidor_dependente.cod_servidor = pensao.cod_servidor\n";
this.queryText += "INNER JOIN (  SELECT cod_pensao\n";
this.queryText += "                   , max(timestamp) as timestamp\n";
this.queryText += "                FROM pessoal.pensao\n";
this.queryText += "               WHERE timestamp <= ultimoTimestampPeriodoMovimentacao("+params["inCodPeriodoMovimentacao"]+", '"+params["stEntidade"]+"')::timestamp \n";
this.queryText += "            GROUP BY cod_pensao) as max_pensao\n";
this.queryText += "        ON pensao.cod_pensao = max_pensao.cod_pensao\n";
this.queryText += "       AND pensao.timestamp  = max_pensao.timestamp\n";
this.queryText += "INNER JOIN pessoal.servidor_contrato_servidor\n";
this.queryText += "        ON servidor_contrato_servidor.cod_servidor = servidor_dependente.cod_servidor\n";
this.queryText += "       AND servidor_contrato_servidor.cod_contrato = registro_evento_periodo.cod_contrato\n";
this.queryText += "INNER JOIN pessoal.servidor\n";
this.queryText += "        ON servidor.cod_servidor = servidor_contrato_servidor.cod_servidor\n";
this.queryText += "INNER JOIN ultimo_contrato_servidor_orgao('"+params["stEntidade"]+"', "+params["inCodPeriodoMovimentacao"]+") as contrato_servidor_orgao\n";
this.queryText += "        ON servidor_contrato_servidor.cod_contrato = contrato_servidor_orgao.cod_contrato\n";


if(params["stTipoFiltro"] == "atributo_servidor_grupo"){
	this.queryText += "INNER JOIN pessoal.atributo_contrato_servidor_valor\n";
	this.queryText += "        ON servidor_contrato_servidor.cod_contrato = atributo_contrato_servidor_valor.cod_contrato\n";
	this.queryText += "INNER JOIN (  SELECT cod_contrato\n";
	this.queryText += "                   , cod_atributo\n";
	this.queryText += "                   , max(timestamp) as timestamp\n";
	this.queryText += "                FROM pessoal.atributo_contrato_servidor_valor\n";
	this.queryText += "            GROUP BY cod_contrato\n";
	this.queryText += "                   , cod_atributo) as max_atributo_contrato_servidor_valor\n";
	this.queryText += "        ON atributo_contrato_servidor_valor.cod_contrato = max_atributo_contrato_servidor_valor.cod_contrato\n";
	this.queryText += "       AND atributo_contrato_servidor_valor.cod_atributo = max_atributo_contrato_servidor_valor.cod_atributo\n";
	this.queryText += "       AND atributo_contrato_servidor_valor.timestamp    = max_atributo_contrato_servidor_valor.timestamp\n";
	this.queryText += "       AND atributo_contrato_servidor_valor.cod_atributo = "+params["inCodAtributo"]+"\n";
	this.queryText += " LEFT JOIN administracao.atributo_valor_padrao\n";
	this.queryText += "        ON atributo_contrato_servidor_valor.valor        = atributo_valor_padrao.valor_padrao\n";
	this.queryText += "       AND atributo_contrato_servidor_valor.cod_atributo = atributo_valor_padrao.cod_atributo\n";
	this.queryText += "       AND atributo_contrato_servidor_valor.cod_modulo   = atributo_valor_padrao.cod_modulo\n";
	this.queryText += "       AND atributo_contrato_servidor_valor.cod_cadastro = atributo_valor_padrao.cod_cadastro\n";
	this.queryText += "       AND atributo_valor_padrao.ativo IS TRUE\n";
}

this.queryText += " LEFT JOIN ultimo_contrato_servidor_local('"+params["stEntidade"]+"', "+params["inCodPeriodoMovimentacao"]+") as contrato_servidor_local\n";
this.queryText += "        ON servidor_contrato_servidor.cod_contrato = contrato_servidor_local.cod_contrato\n";
this.queryText += " LEFT JOIN (SELECT responsavel_legal.*\n";
this.queryText += "              FROM pessoal.responsavel_legal\n";
this.queryText += "                 , (  SELECT cod_pensao\n";
this.queryText += "                           , max(timestamp) as timestamp\n";
this.queryText += "                        FROM pessoal.responsavel_legal\n";
this.queryText += "                       WHERE timestamp <= ultimoTimestampPeriodoMovimentacao("+params["inCodPeriodoMovimentacao"]+", '"+params["stEntidade"]+"')::timestamp  \n";
this.queryText += "                    GROUP BY cod_pensao) as max_responsavel_legal\n";
this.queryText += "             WHERE responsavel_legal.cod_pensao = max_responsavel_legal.cod_pensao\n";
this.queryText += "               AND responsavel_legal.timestamp  = max_responsavel_legal.timestamp) as responsavel_legal\n";
this.queryText += "        ON responsavel_legal.cod_pensao = pensao.cod_pensao\n";
this.queryText += " LEFT JOIN (SELECT pensao_valor.*\n";
this.queryText += "              FROM pessoal.pensao_valor\n";
this.queryText += "                 , (  SELECT cod_pensao\n";
this.queryText += "                           , max(timestamp) as timestamp\n";
this.queryText += "                        FROM pessoal.pensao_valor\n";
this.queryText += "                       WHERE timestamp <= ultimoTimestampPeriodoMovimentacao("+params["inCodPeriodoMovimentacao"]+", '"+params["stEntidade"]+"')::timestamp  \n";
this.queryText += "                    GROUP BY cod_pensao) as max_pensao_valor\n";
this.queryText += "             WHERE pensao_valor.cod_pensao = max_pensao_valor.cod_pensao\n";
this.queryText += "               AND pensao_valor.timestamp  = max_pensao_valor.timestamp) AS pensao_valor\n";
this.queryText += "        ON pensao.cod_pensao = pensao_valor.cod_pensao\n";
this.queryText += "       AND pensao.timestamp  = pensao_valor.timestamp\n";    
this.queryText += " LEFT JOIN (SELECT pensao_banco.*\n";
this.queryText += "              FROM pessoal.pensao_banco\n";
this.queryText += "                 , (  SELECT cod_pensao\n";
this.queryText += "                           , max(timestamp) as timestamp\n";
this.queryText += "                        FROM pessoal.pensao_banco\n";
this.queryText += "                       WHERE timestamp <= ultimoTimestampPeriodoMovimentacao("+params["inCodPeriodoMovimentacao"]+", '"+params["stEntidade"]+"')::timestamp  \n";
this.queryText += "                    GROUP BY cod_pensao) as max_pensao_banco\n";
this.queryText += "             WHERE pensao_banco.cod_pensao = max_pensao_banco.cod_pensao\n";
this.queryText += "               AND pensao_banco.timestamp  = max_pensao_banco.timestamp) AS pensao_banco\n";
this.queryText += "        ON pensao.cod_pensao = pensao_banco.cod_pensao\n";
this.queryText += "       AND pensao.timestamp  = pensao_banco.timestamp\n";         
this.queryText += "  LEFT JOIN monetario.banco\n";
this.queryText += "         ON banco.cod_banco = pensao_banco.cod_banco\n";
this.queryText += "  LEFT JOIN monetario.agencia\n";
this.queryText += "         ON agencia.cod_banco = pensao_banco.cod_banco\n";
this.queryText += "        AND agencia.cod_agencia = pensao_banco.cod_agencia\n";

this.queryText += "    WHERE NOT EXISTS(SELECT 1\n";
this.queryText += "                       FROM pessoal.pensao_excluida\n";
this.queryText += "                      WHERE pensao_excluida.cod_pensao = pensao.cod_pensao\n";
this.queryText += "                        AND pensao_excluida.timestamp = pensao.timestamp)\n";
this.queryText += "      AND registro_evento_periodo.cod_periodo_movimentacao = "+params["inCodPeriodoMovimentacao"]+"\n";

if(params["stTipoFiltro"] == "atributo_servidor_grupo"){
	if(params["boArray"] == "true"){
		this.queryText += " AND atributo_contrato_servidor_valor.valor IN ("+params["stCodigos"]+")\n";
	}else{
		this.queryText += " AND atributo_contrato_servidor_valor.valor = '"+params["stCodigos"]+"'\n";	
	}	
}

if(params["stTipoFiltro"] == "contrato" || params["stTipoFiltro"] == "cgm_contrato"){
	this.queryText += " AND servidor_contrato_servidor.cod_contrato IN ("+params["stCodigos"]+")\n";
}
if(params["stTipoFiltro"] == "lotacao_grupo"){
	this.queryText += " AND contrato_servidor_orgao.cod_orgao IN ("+params["stCodigos"]+")\n";
}
if(params["stTipoFiltro"] == "local_grupo"){
	this.queryText += " AND contrato_servidor_local.cod_local IN ("+params["stCodigos"]+")\n";
}

this.queryText = this.queryText.replaceAll("pessoal", "pessoal" + params["stEntidade"]);
this.queryText = this.queryText.replaceAll("folhapagamento", "folhapagamento" + params["stEntidade"]);

params["query"] = this.queryText;]]></method>
            <property name="dataSource">Data Source</property>
            <xml-property name="queryText"><![CDATA[   SELECT ( SELECT nom_cgm FROM sw_cgm WHERE numcgm = dependente.numcgm ) as nom_cgm_dependente
        , dependente.numcgm as numcgm_dependente
        , ( SELECT nom_cgm FROM sw_cgm WHERE numcgm = servidor.numcgm ) as nom_cgm_servidor
        , (SELECT registro FROM pessoal.contrato WHERE cod_contrato = servidor_contrato_servidor.cod_contrato) as registro
        , recuperaDescricaoOrgao(contrato_servidor_orgao.cod_orgao,(?||'-01-01')::date) as orgao     
        , ( SELECT descricao from organograma.local where local.cod_local = contrato_servidor_local.cod_local ) as local                  
        , pensao.tipo_pensao
        , to_char( pensao.dt_inclusao, 'dd/mm/yyyy' ) as dt_inclusao
        , to_char( pensao.dt_limite,   'dd/mm/yyyy' ) as dt_limite     
        , pensao.percentual
        , pensao.observacao     
        , num_banco
        , nom_banco
        , num_agencia
        , nom_agencia
        , pensao_banco.conta_corrente
        , ( SELECT nom_cgm FROM sw_cgm WHERE numcgm = responsavel_legal.numcgm) as nom_cgm_responsavel
        , responsavel_legal.numcgm as numcgm_responsavel
        , to_real(evento_calculado_dependente.valor)::varchar as valor
]]></xml-property>
