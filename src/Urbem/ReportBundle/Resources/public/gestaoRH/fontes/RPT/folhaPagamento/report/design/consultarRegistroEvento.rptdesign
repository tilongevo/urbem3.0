        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="contratos" id="1149">
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[this.queryText = " select contrato.* \n";
this.queryText += ", sw_cgm.numcgm||'-'||sw_cgm.nom_cgm as cgm\n";
this.queryText += "     , (select descricao from pessoal.cargo where cod_cargo = contrato_servidor.cod_cargo)||'/'||(select descricao from pessoal.cargo where cod_cargo = contrato_servidor_funcao.cod_cargo) as cargo_funcao\n";
this.queryText += "     , (select orgao from organograma.vw_orgao_nivel where vw_orgao_nivel.cod_orgao = contrato_servidor_orgao.cod_orgao)||'-'||recuperaDescricaoOrgao(contrato_servidor_orgao.cod_orgao,('"+params['exercicio']+"-01-01')::date) as orgao\n";
this.queryText += "     , (select descricao from organograma.local where cod_local = contrato_servidor_local.cod_local) as local\n";
this.queryText += "  from pessoal.contrato\n";
this.queryText += "     , pessoal.servidor_contrato_servidor\n";
this.queryText += "     , pessoal.servidor\n";
this.queryText += "     , pessoal.contrato_servidor\n";     
this.queryText += "left join (select contrato_servidor_local.*\n";
this.queryText += "             from pessoal.contrato_servidor_local\n";
this.queryText += "                , (select cod_contrato\n";
this.queryText += "                         , max(timestamp) as timestamp\n";
this.queryText += "                      from pessoal.contrato_servidor_local\n";
this.queryText += "                    group by cod_contrato) as max_contrato_servidor_local\n";             
this.queryText += "            where contrato_servidor_local.cod_contrato = max_contrato_servidor_local.cod_contrato\n";                    
this.queryText += "              and contrato_servidor_local.timestamp = max_contrato_servidor_local.timestamp) as contrato_servidor_local\n";
this.queryText += "       on contrato_servidor_local.cod_contrato = contrato_servidor.cod_contrato     \n";
this.queryText += "     , pessoal.contrato_servidor_funcao\n";
this.queryText += "     , (select cod_contrato\n";
this.queryText += "             , max(timestamp) as timestamp\n";
this.queryText += "          from pessoal.contrato_servidor_funcao\n";
this.queryText += "        group by cod_contrato) as max_contrato_servidor_funcao\n";
this.queryText += "     , pessoal.contrato_servidor_orgao\n";
this.queryText += "     , (select cod_contrato\n";
this.queryText += "             , max(timestamp) as timestamp\n";
this.queryText += "          from pessoal.contrato_servidor_orgao\n";
this.queryText += "        group by cod_contrato) as max_contrato_servidor_orgao\n";        
this.queryText += "     , sw_cgm\n";
this.queryText += " where contrato.cod_contrato = servidor_contrato_servidor.cod_contrato\n";
this.queryText += "   and servidor_contrato_servidor.cod_servidor = servidor.cod_servidor\n";
this.queryText += "   and servidor.numcgm = sw_cgm.numcgm\n";
this.queryText += "   and contrato.cod_contrato = contrato_servidor.cod_contrato\n";
this.queryText += "   and contrato.cod_contrato = contrato_servidor_funcao.cod_contrato\n";
this.queryText += "   and contrato_servidor_funcao.cod_contrato = max_contrato_servidor_funcao.cod_contrato\n";
this.queryText += "   and contrato_servidor_funcao.timestamp = max_contrato_servidor_funcao.timestamp\n";
this.queryText += "   and contrato.cod_contrato = contrato_servidor_orgao.cod_contrato\n";
this.queryText += "   and contrato_servidor_orgao.cod_contrato = max_contrato_servidor_orgao.cod_contrato\n";
this.queryText += "   and contrato_servidor_orgao.timestamp = max_contrato_servidor_orgao.timestamp          \n";   

if(params["inCodConfiguracao"] == "0"){
    this.queryText += "and exists (select 1 \n";
    this.queryText += "                 from folhapagamento.registro_evento_complementar \n";
    this.queryText += "                    , folhapagamento.ultimo_registro_evento_complementar \n";
    this.queryText += "                where registro_evento_complementar.cod_registro = ultimo_registro_evento_complementar.cod_registro \n";
    this.queryText += "                  and registro_evento_complementar.cod_evento = ultimo_registro_evento_complementar.cod_evento \n";
    this.queryText += "                  and registro_evento_complementar.cod_configuracao = ultimo_registro_evento_complementar.cod_configuracao \n";
    this.queryText += "                  and registro_evento_complementar.timestamp = ultimo_registro_evento_complementar.timestamp \n";
    this.queryText += "                  and registro_evento_complementar.cod_contrato = contrato.cod_contrato \n";
    this.queryText += "                  and registro_evento_complementar.cod_complementar = "+params["inCodComplementar"]+" \n";        
    if(params["stTipoFiltro"] == "evento"){
    	this.queryText += "              and registro_evento_complementar.cod_evento IN ("+params["stCodigos"]+") \n";
    }
    this.queryText += "                  and registro_evento_complementar.cod_periodo_movimentacao = "+params["inCodPeriodoMovimentacao"]+") \n";
}
if(params["inCodConfiguracao"] == "1"){
	this.queryText += "and exists (select 1 \n";
	this.queryText += "                 from folhapagamento.registro_evento_periodo \n";
	this.queryText += "                    , folhapagamento.registro_evento \n";
	this.queryText += "                    , folhapagamento.ultimo_registro_evento \n";
	this.queryText += "                where registro_evento_periodo.cod_registro = registro_evento.cod_registro \n";
	this.queryText += "                  and registro_evento.cod_registro = ultimo_registro_evento.cod_registro \n";
	this.queryText += "                  and registro_evento_periodo.cod_contrato = contrato.cod_contrato \n";
    if(params["stTipoFiltro"] == "evento"){
    	this.queryText += "              and registro_evento.cod_evento IN ("+params["stCodigos"]+") \n";
    }	
	this.queryText += "                  and registro_evento_periodo.cod_periodo_movimentacao = "+params["inCodPeriodoMovimentacao"]+") \n";
}
if(params["inCodConfiguracao"] == "2"){
	this.queryText += "and exists (select 1 \n";
	this.queryText += "                 from folhapagamento.registro_evento_ferias \n";
	this.queryText += "                    , folhapagamento.ultimo_registro_evento_ferias \n";
	this.queryText += "                where registro_evento_ferias.cod_registro = ultimo_registro_evento_ferias.cod_registro \n";
	this.queryText += "                  and registro_evento_ferias.cod_evento = ultimo_registro_evento_ferias.cod_evento \n";
	this.queryText += "                  and registro_evento_ferias.desdobramento = ultimo_registro_evento_ferias.desdobramento \n";
	this.queryText += "                  and registro_evento_ferias.timestamp = ultimo_registro_evento_ferias.timestamp \n";
	this.queryText += "                  and registro_evento_ferias.cod_contrato = contrato.cod_contrato \n";
    if(params["stTipoFiltro"] == "evento"){
    	this.queryText += "              and registro_evento_ferias.cod_evento IN ("+params["stCodigos"]+") \n";
    }	
	this.queryText += "                  and registro_evento_ferias.cod_periodo_movimentacao = "+params["inCodPeriodoMovimentacao"]+") \n";
}
if(params["inCodConfiguracao"] == "3"){
    this.queryText += "and exists (select 1 \n";
    this.queryText += "                 from folhapagamento.registro_evento_decimo \n";
    this.queryText += "                    , folhapagamento.ultimo_registro_evento_decimo \n";
    this.queryText += "                where registro_evento_decimo.cod_registro = ultimo_registro_evento_decimo.cod_registro \n";
    this.queryText += "                  and registro_evento_decimo.cod_evento = ultimo_registro_evento_decimo.cod_evento \n";
    this.queryText += "                  and registro_evento_decimo.desdobramento = ultimo_registro_evento_decimo.desdobramento \n";
    this.queryText += "                  and registro_evento_decimo.timestamp = ultimo_registro_evento_decimo.timestamp \n";
    this.queryText += "                  and registro_evento_decimo.cod_contrato = contrato.cod_contrato \n";
    if(params["stTipoFiltro"] == "evento"){
    	this.queryText += "              and registro_evento_decimo.cod_evento IN ("+params["stCodigos"]+") \n";
    }    
    this.queryText += "                  and registro_evento_decimo.cod_periodo_movimentacao = "+params["inCodPeriodoMovimentacao"]+") \n";
}
if(params["inCodConfiguracao"] == "4"){
    this.queryText += "and exists (select 1 \n";
    this.queryText += "                 from folhapagamento.registro_evento_rescisao \n";
    this.queryText += "                    , folhapagamento.ultimo_registro_evento_rescisao \n";
    this.queryText += "                where registro_evento_rescisao.cod_registro = ultimo_registro_evento_rescisao.cod_registro \n";
    this.queryText += "                  and registro_evento_rescisao.cod_evento = ultimo_registro_evento_rescisao.cod_evento \n";
    this.queryText += "                  and registro_evento_rescisao.desdobramento = ultimo_registro_evento_rescisao.desdobramento \n";
    this.queryText += "                  and registro_evento_rescisao.timestamp = ultimo_registro_evento_rescisao.timestamp \n";
    this.queryText += "                  and registro_evento_rescisao.cod_contrato = contrato.cod_contrato \n";
    if(params["stTipoFiltro"] == "evento"){
    	this.queryText += "              and registro_evento_rescisao.cod_evento IN ("+params["stCodigos"]+") \n";
    }    
    this.queryText += "                  and registro_evento_rescisao.cod_periodo_movimentacao = "+params["inCodPeriodoMovimentacao"]+") \n";
}

if(params["stTipoFiltro"] == "contrato" || params["stTipoFiltro"] == "contrato_todos"){
	this.queryText += " AND contrato.cod_contrato IN ("+params["stCodigos"]+") \n";
}

if(params["stTipoFiltro"] == "lotacao"){
	this.queryText += " AND contrato_servidor_orgao.cod_orgao IN ("+params["stCodigos"]+") \n";
}

if(params["stTipoFiltro"] == "local"){
	this.queryText += " AND contrato_servidor_local.cod_local IN ("+params["stCodigos"]+") \n";
}

this.queryText += " order by sw_cgm.nom_cgm";

this.queryText = this.queryText.replaceAll("pessoal", "pessoal" + params["stEntidade"]);
this.queryText = this.queryText.replaceAll("folhapagamento", "folhapagamento" + params["stEntidade"]);

params["query"] = this.queryText;]]></method>
