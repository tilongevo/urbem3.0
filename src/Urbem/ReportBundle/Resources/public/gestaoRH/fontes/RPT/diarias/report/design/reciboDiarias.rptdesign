    <data-sets>
        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="principal" id="1173">
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[this.queryText += "  AND diaria.cod_diaria = "+params['inCodDiaria'];
this.queryText += "  AND diaria.timestamp = '"+params['stTimestamp']+"'";

this.queryText = this.queryText.replaceAll("'' as nom_municipio_prefeitura", "(SELECT nom_municipio FROM sw_municipio WHERE cod_municipio = "+params["inCodMunicipio"]+" AND cod_uf="+params["inCodEstado"]+") as nom_municipio_prefeitura");

this.queryText = this.queryText.replaceAll("pessoal", "pessoal" + params["stEntidade"]);  
this.queryText = this.queryText.replaceAll("diarias", "diarias" + params["stEntidade"]);

params['query'] = this.queryText;]]></method>
            <property name="dataSource">postgres</property>
            <xml-property name="queryText"><![CDATA[SELECT to_char(diaria.dt_inicio, 'dd/mm/yyyy') || ' ('||to_char(diaria.hr_inicio, 'hh24:mi')||'h) a ' || to_char(diaria.dt_termino, 'dd/mm/yyyy')||' ('||to_char(diaria.hr_termino, 'hh24:mi')||'h)' as periodo
     , trim(diaria.motivo) as motivo
     , (SELECT trim(nom_municipio) FROM sw_municipio WHERE sw_municipio.cod_municipio = diaria.cod_municipio AND sw_municipio.cod_uf = diaria.cod_uf ) || '/' || (SELECT sigla_uf FROM sw_uf WHERE sw_uf.cod_uf = diaria.cod_uf) as localidade
     , to_real(diaria.vl_total) || ' (' ||  publico.fn_extenso(diaria.vl_total)  || ')' as valor_total
     , to_real(diaria.quantidade) as quantidade
     , sw_cgm.nom_cgm
     , sw_cgm_pessoa_fisica.cpf
     , trim(trailing 's' from tipo_norma.nom_tipo_norma) || ' - ' || norma.num_norma as norma
     , to_char(norma.dt_assinatura, 'dd/mm/yyyy') as dt_assinatura
     , to_char(norma.dt_publicacao, 'dd/mm/yyyy') as dt_publicacao
     , '' as nom_municipio_prefeitura
     , (SELECT publico.fn_data_extenso(now()::date)) as dt_hoje
     , CASE WHEN empenho.cod_empenho IS NOT NULL 
            THEN (empenho.cod_empenho||'/'||empenho.exercicio)
            ELSE '' 
       END as empenho
     , CASE WHEN nota_liquidacao_paga.timestamp IS NOT NULL 
            THEN (SELECT publico.fn_data_extenso(nota_liquidacao_paga.timestamp::date) )
            ELSE '' 
       END as dt_pagamento_empenho
FROM diarias.diaria
LEFT JOIN diarias.diaria_empenho
     ON (    diaria.cod_diaria   = diaria_empenho.cod_diaria 
         AND diaria.cod_contrato = diaria_empenho.cod_contrato
         AND diaria.timestamp    = diaria_empenho.timestamp)
LEFT JOIN empenho.autorizacao_empenho
     ON (    diaria_empenho.exercicio       = autorizacao_empenho.exercicio
         AND diaria_empenho.cod_entidade    = autorizacao_empenho.cod_entidade
         AND diaria_empenho.cod_autorizacao = autorizacao_empenho.cod_autorizacao)
LEFT JOIN empenho.pre_empenho
     ON (    autorizacao_empenho.cod_pre_empenho = pre_empenho.cod_pre_empenho
         AND autorizacao_empenho.exercicio       = pre_empenho.exercicio)
LEFT JOIN empenho.empenho
     ON (    pre_empenho.cod_pre_empenho    = empenho.cod_pre_empenho
         AND pre_empenho.exercicio          = empenho.exercicio)
LEFT JOIN empenho.nota_liquidacao
     ON (    empenho.cod_empenho            = nota_liquidacao.cod_empenho
         AND empenho.exercicio              = nota_liquidacao.exercicio_empenho
         AND empenho.cod_entidade           = nota_liquidacao.cod_entidade)
LEFT JOIN empenho.nota_liquidacao_paga
     ON (    nota_liquidacao.cod_nota       = nota_liquidacao_paga.cod_nota
         AND nota_liquidacao.exercicio      = nota_liquidacao_paga.exercicio
         AND nota_liquidacao.cod_entidade   = nota_liquidacao_paga.cod_entidade)
   , sw_cgm
   , sw_cgm_pessoa_fisica
   , normas.norma
   , normas.tipo_norma
   , pessoal.servidor
   , pessoal.contrato
   , pessoal.servidor_contrato_servidor
WHERE diaria.cod_contrato = contrato.cod_contrato
  AND contrato.cod_contrato = servidor_contrato_servidor.cod_contrato
  AND servidor_contrato_servidor.cod_servidor = servidor.cod_servidor
  AND servidor.numcgm = sw_cgm.numcgm
  AND sw_cgm.numcgm = sw_cgm_pessoa_fisica.numcgm
  AND diaria.cod_norma = norma.cod_norma
  AND norma.cod_tipo_norma = tipo_norma.cod_tipo_norma
]]></xml-property>
        </oda-data-set>
    </data-sets>
