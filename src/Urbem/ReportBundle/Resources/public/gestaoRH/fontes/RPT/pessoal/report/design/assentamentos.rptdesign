        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="assentamentos" id="1149">
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[this.queryText  = "    SELECT cadastro.nom_cgm\n";
this.queryText += "         , cadastro.registro\n";
this.queryText += "         , (SELECT descricao FROM pessoal.classificacao_assentamento WHERE cod_classificacao = assentamento_assentamento.cod_classificacao)||'/'||assentamento_assentamento.descricao as classificacao_assentamento\n";
this.queryText += "         , CASE WHEN assentamento_gerado.periodo_inicial IS NULL \n";
this.queryText += "                THEN to_char(assentamento_gerado.periodo_final,'dd/mm/yyyy') \n";
this.queryText += "                WHEN assentamento_gerado.periodo_final IS NULL \n";
this.queryText += "                THEN to_char(assentamento_gerado.periodo_inicial,'dd/mm/yyyy') \n";
this.queryText += "                ELSE to_char(assentamento_gerado.periodo_inicial,'dd/mm/yyyy')||' a '||to_char(assentamento_gerado.periodo_final,'dd/mm/yyyy') \n"; 
this.queryText += "           END AS periodo \n";
this.queryText += "         , CASE WHEN (assentamento_gerado.periodo_final - assentamento_gerado.periodo_inicial + 1) > 9 \n";
this.queryText += "                THEN CAST((assentamento_gerado.periodo_final - assentamento_gerado.periodo_inicial + 1) AS VARCHAR) \n";
this.queryText += "	               ELSE '0'||(assentamento_gerado.periodo_final - assentamento_gerado.periodo_inicial + 1) \n";
this.queryText += "           END AS dias \n";
this.queryText += "         , (SELECT num_norma||'/'||exercicio||' - '||descricao FROM normas.norma WHERE cod_norma = assentamento_assentamento.cod_norma) as norma\n";
this.queryText += "         , assentamento_gerado.observacao\n";
this.queryText += "         , (select valor from administracao.configuracao where parametro = 'dtContagemInicial' and exercicio = '"+params["exercicio"]+"') as contagem_tempo\n";
this.queryText += "         , to_char(cadastro.dt_posse,'dd/mm/yyyy') as dt_posse\n";
this.queryText += "         , to_char(cadastro.dt_nomeacao,'dd/mm/yyyy') as dt_nomeacao\n";
this.queryText += "         , to_char(cadastro.dt_admissao,'dd/mm/yyyy') as dt_admissao\n";
this.queryText += "         , cadastro.desc_regime_funcao as regime\n";
this.queryText += "         , cadastro.desc_sub_divisao_funcao as sub_divisao\n";
this.queryText += "         , cadastro.desc_funcao as funcao\n";
this.queryText += "         , cadastro.desc_especialidade_funcao as especialidade\n";     
this.queryText += "      FROM pessoal.assentamento_gerado\n";
this.queryText += "         , (  SELECT cod_assentamento_gerado\n";
this.queryText += "                   , max(timestamp) as timestamp\n";
this.queryText += "                FROM pessoal.assentamento_gerado\n";
this.queryText += "            GROUP BY cod_assentamento_gerado) as max_assentamento_gerado\n";
this.queryText += "         , pessoal.assentamento_assentamento\n";
this.queryText += "         , pessoal.assentamento_gerado_contrato_servidor\n";
this.queryText += "         , (\n"; 
this.queryText += "            SELECT * FROM recuperarContratoServidor('cgm,f,ef,rf,sf,o,oo,l,anp','"+params["stEntidade"]+"',0,'"+params['stTipoFiltro']+"','"+params['stCodigos']+"','"+params["exercicio"]+"')\n"; 
this.queryText += "           ) as cadastro\n";
this.queryText += "     WHERE assentamento_gerado.cod_assentamento_gerado = max_assentamento_gerado.cod_assentamento_gerado\n";
this.queryText += "       AND assentamento_gerado.timestamp = max_assentamento_gerado.timestamp\n";
this.queryText += "       AND assentamento_gerado.cod_assentamento = assentamento_assentamento.cod_assentamento\n";
this.queryText += "       AND assentamento_gerado.cod_assentamento_gerado = assentamento_gerado_contrato_servidor.cod_assentamento_gerado\n";
this.queryText += "       AND assentamento_gerado_contrato_servidor.cod_contrato = cadastro.cod_contrato\n";
this.queryText += "       AND NOT EXISTS (SELECT 1\n";
this.queryText += "                         FROM pessoal.assentamento_gerado_excluido\n";
this.queryText += "                        WHERE assentamento_gerado.cod_assentamento_gerado = assentamento_gerado_excluido.cod_assentamento_gerado \n";
this.queryText += "                          AND assentamento_gerado.timestamp = assentamento_gerado_excluido.timestamp\n";
this.queryText += "                         )\n";

this.queryText += "       AND assentamento_gerado.cod_assentamento IN ("+params["stCodAssentamentos"]+")\n";

if(params["stPeriodoInicial"] != "" &amp;&amp; params["stPeriodoFinal"] != ""){
	this.queryText += "  AND ((assentamento_gerado.periodo_inicial between to_date('"+params["stPeriodoInicial"]+"','dd-mm-yyyy') AND to_date('"+params["stPeriodoFinal"]+"','dd-mm-yyyy'))   \n";
	this.queryText += "    OR (assentamento_gerado.periodo_final between to_date('"+params["stPeriodoInicial"]+"','dd-mm-yyyy') AND to_date('"+params["stPeriodoFinal"]+"','dd-mm-yyyy')))    \n";	
}

this.queryText += " ORDER BY periodo_inicial \n";
this.queryText += "        , periodo_final\n";

this.queryText = this.queryText.replaceAll("pessoal", "pessoal" + params["stEntidade"]);
this.queryText = this.queryText.replaceAll("folhapagamento", "folhapagamento" + params["stEntidade"]);
params["query"] = this.queryText;]]></method>
