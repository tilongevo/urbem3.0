            <xml-property name="queryText"><![CDATA[     SELECT consignacao_emprestimo_banrisul.num_linha
          , pessoal.contrato.registro as matricula
          , consignacao_emprestimo_banrisul.nom_funcionario
          , consignacao_emprestimo_banrisul.cod_canal
          , consignacao_emprestimo_banrisul.prestacao
          ,to_number(
                    SUBSTR(
                            to_char(consignacao_emprestimo_banrisul.val_consignar,'999999999999999'),
                            1,
                            length(to_char(consignacao_emprestimo_banrisul.val_consignar,'999999999999999')) - 2)
                    ||'.'||
                    SUBSTR(
                            to_char(consignacao_emprestimo_banrisul.val_consignar,'999999999999999'),
                            length(to_char(consignacao_emprestimo_banrisul.val_consignar,'999999999999999'))-1,2),
                            '9999999999999.99'
                          )
            AS val_consignar
          ,to_number(
                    SUBSTR(
                            to_char(consignacao_emprestimo_banrisul.val_consignado,'999999999999999'),
                            1,
                            length(to_char(consignacao_emprestimo_banrisul.val_consignado,'999999999999999')) - 2)
                    ||'.'||
                    SUBSTR(
                            to_char(consignacao_emprestimo_banrisul.val_consignado,'999999999999999'),
                            length(to_char(consignacao_emprestimo_banrisul.val_consignado,'999999999999999'))-1,2),
                            '9999999999999.99'
                          )
            AS val_consignado
          , consignacao_emprestimo_banrisul.cod_contrato
          , consignacao_emprestimo_banrisul.cod_periodo_movimentacao
          , consignacao_emprestimo_banrisul.origem_pagamento
          , motivos_rejeicao_consignacao_emprestimo_banrisul.cod_motivo_rejeicao
          , motivos_rejeicao_consignacao_emprestimo_banrisul.descricao as descricao_rejeicao
       FROM ima.consignacao_emprestimo_banrisul
  LEFT JOIN ima.consignacao_emprestimo_banrisul_erro
         ON consignacao_emprestimo_banrisul.num_linha = consignacao_emprestimo_banrisul_erro.num_linha
        AND consignacao_emprestimo_banrisul.cod_periodo_movimentacao = consignacao_emprestimo_banrisul_erro.cod_periodo_movimentacao
  LEFT JOIN ima.motivos_rejeicao_consignacao_emprestimo_banrisul
         ON motivos_rejeicao_consignacao_emprestimo_banrisul.cod_motivo_rejeicao = consignacao_emprestimo_banrisul_erro.cod_motivo_rejeicao
 LEFT JOIN pessoal.contrato
        ON contrato.cod_contrato = consignacao_emprestimo_banrisul.cod_contrato]]></xml-property>
