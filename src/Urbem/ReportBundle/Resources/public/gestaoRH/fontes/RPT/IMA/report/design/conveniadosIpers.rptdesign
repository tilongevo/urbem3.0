            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[this.queryText  = "SELECT to_char(dt_situacao, 'dd/mm/yyyy') as dt_situacao, valor::NUMERIC,* \n";
this.queryText += "FROM    													   \n";
this.queryText += " exportarArquivoIpers( 									   \n";
this.queryText += params["inCodPeriodoMovimentacao"]+",                        \n";
this.queryText += "'"+params["stEntidade"]+"',								   \n";
this.queryText += "'"+params["stExercicio"]+"',								   \n";
this.queryText += "'"+params["stTipoFiltro"]+"',							   \n";
this.queryText += "'"+params["stValoresFiltro"]+"',                            \n";
this.queryText += "'"+params["stSituacaoCadastro"]+"',                         \n";
this.queryText += ""+params["inCodTipoEmissao"]+",                             \n";
this.queryText += params["inCodFolha"]+",                                      \n";
this.queryText += "'"+params["stDesdobramento"]+"',                            \n";
this.queryText += params["inCodComplementar"]+",                               \n";
this.queryText += params["boAgruparFolhas"]+"                                  \n";
this.queryText += " ) as contrato ORDER BY nom_cgm \n";

params["query"] = this.queryText;]]></method>
            <property name="dataSource">postgres</property>
            <list-property name="resultSet"/>
            <xml-property name="queryText"><![CDATA[]]></xml-property>
        </oda-data-set>
