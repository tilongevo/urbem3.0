                </structure>
            </list-property>
            <xml-property name="queryText"><![CDATA[     SELECT to_char(escala_turno.dt_turno, 'dd/mm/yyyy') as dt_turno
          , to_char(escala_turno.hora_entrada_1, 'hh24:mi') as hora_entrada_1
          , to_char(escala_turno.hora_saida_1, 'hh24:mi') as hora_saida_1
          , to_char(escala_turno.hora_entrada_2, 'hh24:mi') as hora_entrada_2
          , to_char(escala_turno.hora_saida_2, 'hh24:mi') as hora_saida_2
          , (CASE WHEN escala_turno.tipo = 'F' THEN 'Folga'
                  WHEN escala_turno.tipo = 'T' THEN 'Trabalho'
              END) as tipo_formatado
       FROM ponto.escala
 INNER JOIN ponto.escala_turno
         ON (    escala.cod_escala = escala_turno.cod_escala
             AND escala.ultimo_timestamp = escala_turno.timestamp)
      WHERE NOT EXISTS ( SELECT 1
                           FROM ponto.escala_exclusao
                          WHERE escala.cod_escala = escala_exclusao.cod_escala )
        AND escala.cod_escala = ?
   ORDER BY escala_turno.dt_turno   ]]></xml-property>
