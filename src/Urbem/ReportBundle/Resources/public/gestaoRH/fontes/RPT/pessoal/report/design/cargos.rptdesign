            <xml-property name="queryText"><![CDATA[  SELECT nivel_padrao_nivel.cod_nivel_padrao
       , nivel_padrao_nivel.cod_padrao
       , nivel_padrao_nivel.descricao
       , nivel_padrao_nivel.valor
       , 'R$ '||coalesce(to_real(nivel_padrao_nivel.valor), '0,00') as valor_formatado
       , nivel_padrao_nivel.percentual
       , coalesce(to_real(nivel_padrao_nivel.percentual), '0,00')||' %' as percentual_formatado
       , nivel_padrao_nivel.qtdmeses
       , nivel_padrao_nivel.timestamp
    FROM folhapagamento.nivel_padrao_nivel
       , (  SELECT cod_padrao
                 , max(timestamp) as timestamp_padrao
