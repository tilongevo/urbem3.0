    this.queryText += "                                                                                               WHERE cod_periodo_movimentacao = (";
    this.queryText += "                                                                                                                                  SELECT MAX(cod_periodo_movimentacao)";
    this.queryText += "                                                                                                                                    FROM folhapagamento.periodo_movimentacao";
    this.queryText += "                                                                                                                                )";
    this.queryText += "                                                                                            )";
    this.queryText += "                                                          AND aposentadoria_excluida.cod_contrato IS NULL";
    this.queryText += "                                                     GROUP BY aposentadoria.cod_contrato ";
    this.queryText += "                                                   ) AS interna";
    this.queryText += "                                          GROUP BY interna.cod_contrato";
    this.queryText += "                                        ) AS aposentado";
    this.queryText += "                                    ON aposentado.cod_contrato = contrato.cod_contrato";
    this.queryText += "                              LEFT JOIN (";
    this.queryText += "                                            SELECT contrato_servidor_caso_causa.cod_contrato";
    this.queryText += "                                                 , count(*) as total";
    this.queryText += "                                              FROM pessoal.contrato_servidor_caso_causa";
    this.queryText += "                                             WHERE dt_rescisao <= (";
    this.queryText += "                                                                    SELECT periodo_movimentacao.dt_final";
    this.queryText += "                                                                      FROM folhapagamento.periodo_movimentacao";
    this.queryText += "                                                                     WHERE cod_periodo_movimentacao = (";
    this.queryText += "                                                                                                        SELECT MAX(cod_periodo_movimentacao)";
    this.queryText += "                                                                                                          FROM folhapagamento.periodo_movimentacao";
    this.queryText += "                                                                                                      )";
    this.queryText += "                                                                  )";
    this.queryText += "                                          GROUP BY contrato_servidor_caso_causa.cod_contrato";
    this.queryText += "                                        ) AS rescindido";
    this.queryText += "                                     ON rescindido.cod_contrato = contrato.cod_contrato   ";
    this.queryText += "                         ) AS situacao_contrato 		";
    this.queryText += "                      ON situacao_contrato.cod_contrato = contrato_servidor.cod_contrato";
    this.queryText += "                   WHERE situacao_contrato.status = 'A' ";
    this.queryText += "                     AND (";
	this.queryText += "			                 	( contrato_servidor_especialidade_cargo.cod_especialidade != contrato_servidor_especialidade_funcao.cod_especialidade";
    this.queryText += "                               OR contrato_servidor_especialidade_cargo.cod_especialidade IS NULL";
    this.queryText += "                             )";
    this.queryText += "                       		OR contrato_servidor.cod_sub_divisao != contrato_servidor_sub_divisao_funcao.cod_sub_divisao";
    this.queryText += "                             OR contrato_servidor.cod_regime      != contrato_servidor_regime_funcao.cod_regime";
    this.queryText += "                          )";
	this.queryText += "		      GROUP BY contrato_servidor_especialidade_funcao.cod_especialidade";
	this.queryText += "			         , contrato_servidor_sub_divisao_funcao.cod_sub_divisao ";
