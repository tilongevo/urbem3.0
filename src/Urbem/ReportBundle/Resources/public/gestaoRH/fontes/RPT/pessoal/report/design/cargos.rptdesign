            <xml-property name="queryText"><![CDATA[      SELECT reajuste.cod_reajuste
           , CASE WHEN reajuste.percentual IS NOT NULL THEN
	           coalesce(translate(reajuste.percentual::VARCHAR,'.',','), '0,00')||' %'
	           ELSE
	           'R$'||coalesce(to_real(reajuste.valor_absoluto), '0,00')
             END as reajuste_formatado
           , CASE WHEN reajuste.percentual IS NOT NULL THEN
           	   'P'
           	   ELSE
           	   'V'
           	 END as tipo_reajuste
           , to_char(reajuste.dt_reajuste, 'dd/mm/yyyy') as dt_reajuste
           , (          SELECT coalesce('R$'||to_real(padrao_padrao.valor), '')
                          FROM folhapagamento.padrao_padrao
                    INNER JOIN (  SELECT cod_padrao
                                       , max(timestamp) as timestamp
                                    FROM folhapagamento.reajuste_padrao_padrao
                                GROUP BY cod_padrao) as max_reajuste_padrao_padrao
                            ON padrao_padrao.cod_padrao = max_reajuste_padrao_padrao.cod_padrao
                           AND padrao_padrao.timestamp = max_reajuste_padrao_padrao.timestamp
                         WHERE padrao_padrao.cod_padrao = reajuste_padrao_padrao.cod_padrao
             ) as valor
           , (
                  SELECT assentamento_gerado.observacao
                    FROM pessoal.assentamento_gerado
              INNER JOIN pessoal.assentamento_gerado_contrato_servidor
                      ON assentamento_gerado_contrato_servidor.cod_assentamento_gerado = assentamento_gerado.cod_assentamento_gerado
              INNER JOIN folhapagamento.reajuste_contrato_servidor_salario
                      ON assentamento_gerado_contrato_servidor.cod_contrato = reajuste_contrato_servidor_salario.cod_contrato
              INNER JOIN (      SELECT cod_contrato
                                     , cod_reajuste
                                     , max(timestamp) as timestamp
                                  FROM folhapagamento.reajuste_contrato_servidor_salario
                              GROUP BY cod_contrato
                                     , cod_reajuste
                         ) as max_reajuste_contrato_servidor_salario
                      ON max_reajuste_contrato_servidor_salario.cod_contrato = reajuste_contrato_servidor_salario.cod_contrato
                     AND max_reajuste_contrato_servidor_salario.cod_reajuste = reajuste_contrato_servidor_salario.cod_reajuste
                     AND max_reajuste_contrato_servidor_salario.timestamp = reajuste_contrato_servidor_salario.timestamp
              INNER JOIN folhapagamento.reajuste
                      ON reajuste_contrato_servidor_salario.cod_reajuste = reajuste.cod_reajuste
                   WHERE reajuste.cod_reajuste = reajuste_padrao_padrao.cod_reajuste
                     AND assentamento_gerado.timestamp = reajuste_padrao_padrao.timestamp
                   LIMIT 1

           ) as observacao
        FROM (   SELECT reajuste.*
                      , reajuste_percentual.valor as percentual
                      , reajuste_absoluto.valor as valor_absoluto
                   FROM folhapagamento.reajuste
              LEFT JOIN folhapagamento.reajuste_percentual 
                     ON reajuste.cod_reajuste = reajuste_percentual.cod_reajuste
              LEFT JOIN folhapagamento.reajuste_absoluto
                     ON reajuste.cod_reajuste = reajuste_absoluto.cod_reajuste
             ) as reajuste
  INNER JOIN folhapagamento.reajuste_padrao_padrao
          ON reajuste.cod_reajuste = reajuste_padrao_padrao.cod_reajuste
         AND reajuste.origem = 'P'
       WHERE reajuste.cod_reajuste NOT IN (SELECT cod_reajuste FROM folhapagamento.reajuste_exclusao)
         AND reajuste_padrao_padrao.cod_padrao = ?
    ORDER BY dt_reajuste DESC]]></xml-property>
