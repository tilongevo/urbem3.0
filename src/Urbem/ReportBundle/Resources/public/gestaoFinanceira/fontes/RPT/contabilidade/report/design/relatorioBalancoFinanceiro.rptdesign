        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="assinaturas" id="3533">
            <list-property name="parameters">
                <structure>
                    <property name="name">exercicio</property>
                    <property name="paramName">exercicio</property>
                    <property name="dataType">string</property>
                    <property name="position">1</property>
                    <property name="isInput">true</property>
                    <property name="isOutput">false</property>
                </structure>
            </list-property>
            <structure name="cachedMetaData"/>
            <method name="beforeOpen"><![CDATA[if (params['timestamp_assinatura'] != "") {
	this.queryText += ' AND assinatura.timestamp IN (' + params['timestamp_assinatura'] + ')';
}

if (params['timestamp_assinatura'] != "") {
	this.queryText += ' AND assinatura.cod_entidade IN (' + params['entidade_assinatura'] + ')';
}

if (params['numcgm_assinatura'] != "") {
	this.queryText += ' AND assinatura.numcgm IN (' + params['numcgm_assinatura'] + ')';
}]]></method>
            <property name="dataSource">postgres</property>
            <xml-property name="queryText"><![CDATA[SELECT  assinatura.exercicio
	,	assinatura.cod_entidade
	,	assinatura.numcgm
	,	assinatura.cargo
	,	assinatura.timestamp
	,	sw_cgm.nom_cgm
FROM administracao.assinatura
INNER JOIN sw_cgm 
		ON assinatura.numcgm = sw_cgm.numcgm
WHERE assinatura.exercicio = ?]]></xml-property>
        </oda-data-set>
        <oda-data-set extensionID="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet" name="dsNotasExplicativa" id="6906">
            <list-property name="parameters">
                <structure>
                    <property name="name">cod_acao</property>
                    <property name="paramName">cod_acao</property>
                    <property name="dataType">integer</property>
                    <property name="position">1</property>
                    <property name="isInput">true</property>
                    <property name="isOutput">false</property>
                </structure>
                <structure>
                    <property name="name">data_nota_inicial</property>
                    <property name="paramName">data_inicial_nota</property>
                    <property name="dataType">date</property>
                    <property name="position">2</property>
                    <property name="isInput">true</property>
                    <property name="isOutput">false</property>
                </structure>
                <structure>
                    <property name="name">data_nota_final</property>
                    <property name="paramName">data_final_nota</property>
                    <property name="dataType">date</property>
                    <property name="position">3</property>
                    <property name="isInput">true</property>
                    <property name="isOutput">false</property>
                </structure>
            </list-property>
            <structure name="cachedMetaData"/>
            <property name="dataSource">postgres</property>
            <xml-property name="queryText"><![CDATA[SELECT nota_explicativa.cod_acao
	 , nota_explicativa.nota_explicativa
     , to_char(to_date(nota_explicativa.dt_inicial::varchar, 'yyyy-mm-dd'), 'dd/mm/yyyy')
    || ' até ' || to_char(to_date(nota_explicativa.dt_final::varchar, 'yyyy-mm-dd'), 'dd/mm/yyyy') as periodo
     , acao.nom_acao
  FROM contabilidade.nota_explicativa
  JOIN contabilidade.nota_explicativa_acao
    ON nota_explicativa.cod_acao = nota_explicativa_acao.cod_acao
  JOIN administracao.acao
    ON acao.cod_acao = nota_explicativa_acao.cod_acao
 WHERE true
   AND nota_explicativa.cod_acao = ?
   AND nota_explicativa.dt_inicial <= ?
   AND nota_explicativa.dt_final >= ?]]></xml-property>
        </oda-data-set>
    </data-sets>
