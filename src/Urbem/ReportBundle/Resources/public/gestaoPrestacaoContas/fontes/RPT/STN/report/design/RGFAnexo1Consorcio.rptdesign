            <xml-property name="queryText"><![CDATA[SELECT nivel, cod_estrutural, descricao, liquidado, restos
FROM stn.fn_rgf_anexo1_despesas ( ? , ? , ? , ? ) 
as retorno (
        nivel                   INTEGER,
        cod_estrutural          VARCHAR,
        descricao               VARCHAR,
        liquidado  DECIMAL(14,2),  
        restos  DECIMAL(14,2)    
) 
WHERE cod_estrutural <> '3.1.2.0.00.00.00.00.00'
  AND cod_estrutural <> '3.2.4.0.00.00.00.00.00'
]]></xml-property>
